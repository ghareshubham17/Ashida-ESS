{
  "expo": {
    "name": "AshidaESS",
    "slug": "AshidaESS",
    "version": "1.0.0",
    "owner": "shubham17g",
    "extra": {
      "eas": {
        "projectId": "20ad97f9-fe68-4f4a-b5ee-25e1ae8e418d"
      }
    },
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "ashidaess",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/logo.png"
      },
      "versionCode": 5,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.shubham17g.AshidaESS",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow AshidaESS to use your location for attendance check-in/check-out.",
          "locationAlwaysPermission": "Allow AshidaESS to use your location for attendance check-in/check-out.",
          "locationWhenInUsePermission": "Allow AshidaESS to use your location for attendance check-in/check-out."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
